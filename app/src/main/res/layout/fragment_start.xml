<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StartFragment">

<!--     id без camel_Case -->
    <EditText
        android:id="@+id/edit_Max_Generate_Number" 
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="225dp"
        android:layout_marginTop="250dp"
        android:ems="10"
        android:hint="@string/max_generate_value"
        android:inputType="number"
        android:minHeight="48dp" />

    <EditText
        android:id="@+id/edit_Min_Generate_Number"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="250dp"
        android:ems="10"
        android:hint="@string/min_generate_value"
        android:inputType="number"
        android:minHeight="48dp" />

    <Button
        android:id="@+id/button_Generate_Number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="400dp"
        android:text="@string/generate" />

    <TextView
        android:id="@+id/last_Generate_Number_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="185dp"
        android:layout_marginTop="500dp"
        android:text="" />

    <TextView
        android:id="@+id/validation_Text_View"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="" />
</FrameLayout>
